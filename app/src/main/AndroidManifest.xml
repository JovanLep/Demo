    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".ui.Activity.MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".ui.Activity.Main2Activity" />
        <activity android:name=".ui.Activity.NinePointActivity" />
        <activity
            android:name=".ui.Activity.LoadingActivity"
            android:launchMode="standard">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.Activity.MobActivity" />
        <activity android:name=".ui.Activity.SuanMActivity" />
        <activity android:name=".ui.Activity.SuanResultActivity" />
        <activity android:name=".ui.Activity.Game2048Activity"></activity>
    </application>

</manifest>